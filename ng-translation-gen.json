{
  "$schema": "./node_modules/ng-translation-gen/ng-translation-gen-schema.json",
  "input": "src/locale",
  "output": "src/app/messages",
  "mapping": {
    "cyclos4-ui": "Messages"
  },
  "argumentType": "string | number",
  "defaultLocale": "en",
  "locales": [
    "en",
    "es",
    "nl",
    "pt_BR",
    "fr"
  ]
}