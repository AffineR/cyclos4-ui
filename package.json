{
  "name": "cyclos4-ui",
  "displayName": "Cyclos 4 UI",
  "description": "The new Cyclos 4 frontend",
  "icon": "cyclos.png",
  "version": "4.15.5",
  "license": "MIT",
  "author": {
    "name": "Cyclos development team",
    "email": "info@cyclos.org",
    "url": "https://www.cyclos.org"
  },
  "scripts": {
    "ng": "ng",
    "ng-openapi-gen": "ng-openapi-gen",
    "ng-translation-gen": "ng-translation-gen",
    "merge-translations": "ng-translation-gen --merge",
    "generate-icons": "node tools/generate-icons.js",
    "generate": "ng-openapi-gen && ng-translation-gen && npm run generate-icons",
    "prepare-build": "npm run generate",
    "start": "npm run generate && ng serve --host 0.0.0.0 --proxy-config proxy.json",
    "build": "npm run prepare-build && npm run ng-build",
    "analyze": "npm run prepare-build && ng build --configuration production --stats-json && webpack-bundle-analyzer --port 9200 dist/ui/stats.json",
    "lint": "npm run ng -- lint",
    "ng-build": "ng build --configuration production",
    "ng-build-dev": "ng build --source-map=false",
    "ng-build-dev-sm": "ng build --source-map=true"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/build-angular": "^13.3.5",
    "@angular/animations": "^13.3.8",
    "@angular/cli": "^13.3.5",
    "@angular/common": "^13.3.8",
    "@angular/compiler": "^13.3.8",
    "@angular/compiler-cli": "^13.3.8",
    "@angular/core": "^13.3.8",
    "@angular/forms": "^13.3.8",
    "@angular/language-service": "^13.3.8",
    "@angular/localize": "^13.3.8",
    "@angular/platform-browser": "^13.3.8",
    "@angular/platform-browser-dynamic": "^13.3.8",
    "@angular/router": "^13.3.8",
    "@angular/service-worker": "^13.3.8",
    "@swc/core": ">=1.2.50",
    "@swc/wasm": ">=1.2.50",
    "@types/autosize": "^4.0.1",
    "@types/downloadjs": "^1.4.3",
    "@types/google-maps": "^3.2.3",
    "@types/lodash-es": "^4.17.6",
    "@types/lscache": "^1.3.1",
    "@types/markerclustererplus": "^2.1.33",
    "@types/node": "^17.0.33",
    "codelyzer": "^0.0.26",
    "core-js": "^3.22.5",
    "file-loader": "^6.2.0",
    "karma": "^6.3.20",
    "ng-openapi-gen": "^0.21.2",
    "ng-packagr": "^13.3.1",
    "ng-translation-gen": "^0.6.1",
    "postcss": "^8.4.13",
    "protractor": "^7.0.0",
    "rxjs": "^7.5.5",
    "tailwindcss": "^3.0.24",
    "tar": "^6.1.11",
    "ts-node": "^10.7.0",
    "tslib": "^2.4.0",
    "tslint": "^6.1.3",
    "typescript": "^4.6.4",
    "upup": "^1.1.0",
    "webpack": "^4.0.0 || ^5.0.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "xmldom": "^0.6.0",
    "zone.js": "^0.11.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.1.2",
    "@angular/cli": "^13.1.2",
    "@angular/compiler-cli": "^13.1.1",
    "@angular/language-service": "^13.1.1",
    "@types/autosize": "^4.0.1",
    "@types/downloadjs": "^1.4.3",
    "@types/google-maps": "^3.2.3",
    "@types/js-cookie": "^3.0.1",
    "@types/lodash-es": "^4.17.5",
    "@types/lscache": "^1.3.1",
